msgstr "  -n                        suprime nova linha final\n"

#: src/gettext.c:270 src/ngettext.c:234
#, c-format
msgid "  -E                        (ignored for compatibility)\n"
msgstr "  -E                        (ignorado para compatibilidade)\n"

